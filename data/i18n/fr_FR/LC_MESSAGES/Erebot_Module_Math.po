msgid   "^ is undefined for negative exponents"
msgstr  "L'opérateur ^ n'est pas défini avec des exposants négatifs"
#
msgid   "Syntax error"
msgstr  "Erreur de syntaxe"
#
# File: src/Erebot/Module/Math.php, line: 87
msgid   "Provides the <b><var name=\"trigger\"/></b> command which allows you "
        "to submit formulae to the bot for computation."
msgstr  "Fournit la commande <b><var name=\"trigger\"/></b> permettant "
        "de soumettre une formule au bot pour calcul."
#
msgid   "Exponent is too big for computation"
msgstr  "L'exposant est trop grand pour ce calcul"
#
msgid   "Division by zero"
msgstr  "Division par zéro"
#
msgid   "Could not register Math trigger"
msgstr  "Impossible d'enregistrer le déclencheur pour le module Math"
#
# File: src/Erebot/Module/Math.php, line: 101
msgid   "<b>Usage:</b> !<var name='trigger'/> &lt;<u>formula</u>&gt;. Computes "
        "the given formula and displays the result. The four basic operators "
        "(+, -, *, /), parenthesis, exponentiation (^) and modules (%) are "
        "supported."
msgstr  "<b>Usage :</b> !<var name='trigger'/> &lt;<u>formule</u>&gt;. "
        "Calcule la formule donnée et affiche le résultat.. "
        "Les quatres opérateurs basics (+, -, *, /), les parenthèses, la "
        "puissance (^) et le modulo (%) sont supportés."
#
msgid   "% is undefined on real numbers"
msgstr  "L'opérateur % n'est pas défini sur les réels"
